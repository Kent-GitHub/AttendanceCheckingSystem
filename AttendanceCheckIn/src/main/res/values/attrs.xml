<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="indicator_icon" format="reference" />
    <attr name="indicator_color" format="color" />
    <attr name="indicator_text" format="string" />
    <attr name="indicator_textSize" format="dimension" />

    <attr name="textLeft" format="string" />
    <attr name="textLeftMargin" format="dimension" />
    <attr name="textRight" format="string" />
    <attr name="imageLeftRes" format="integer" />
    <attr name="topLine" format="boolean" />
    <attr name="from" format="float" />
    <attr name="to" format="float" />

    <attr name="strokeLeft" format="boolean" />
    <attr name="strokeTop" format="boolean" />
    <attr name="strokeRight" format="boolean" />
    <attr name="strokeBottom" format="boolean" />

    <declare-styleable name="IndicatorView">
        <attr name="indicator_icon" />
        <attr name="indicator_color" />
        <attr name="indicator_text" />
        <attr name="indicator_textSize" />
    </declare-styleable>

    <declare-styleable name="SettingViewFroward">
        <attr name="textLeft" />
        <attr name="textLeftMargin" />
        <attr name="textRight" />
        <attr name="imageLeftRes" />
        <attr name="topLine" />
        <attr name="from"/>
        <attr name="to" />
    </declare-styleable>

    <declare-styleable name="StrokeTextView">
        <attr name="strokeLeft" />
        <attr name="strokeTop" />
        <attr name="strokeRight" />
        <attr name="strokeBottom" />
    </declare-styleable>

    <declare-styleable name="StrokeLinearLayout">
        <attr name="strokeLeft" />
        <attr name="strokeTop" />
        <attr name="strokeRight" />
        <attr name="strokeBottom" />
    </declare-styleable>

    <declare-styleable name="StrokeButton">
        <attr name="strokeLeft" />
        <attr name="strokeTop" />
        <attr name="strokeRight" />
        <attr name="strokeBottom" />
    </declare-styleable>

</resources>